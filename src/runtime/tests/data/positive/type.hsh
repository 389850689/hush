std.assert(std.type(nil) == "nil")
std.assert(std.type(true) == "bool")
std.assert(std.type(1) == "int")
std.assert(std.type(1.0) == "float")
std.assert(std.type('\0') == "char")
std.assert(std.type("\n") == "string")
std.assert(std.type([]) == "array")
std.assert(std.type(@[]) == "dict")
std.assert(std.type(function () end) == "function")
std.assert(std.type(std.error("error", nil)) == "error")
